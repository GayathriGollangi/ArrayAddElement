import java.util.Scanner;
import java.util.ArrayList;
class Main{
    public static void main (String[] args) {
        /* code */
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        ArrayList<Integer> a = new ArrayList<>();
        a.add(2);
        a.add(4);
        a.add(6);
        a.add(8);
        a.add(10);
        a.add(12);
        a.add(n);
        System.out.println(a);
    }
}
